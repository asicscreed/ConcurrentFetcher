{
    "opts": {
        "encoding": "utf8",
        "destination": "./docs/"
    },
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": "./dist",
        "includePattern": "concurrentfetcher.js"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true,
            "includeDate": false
        }
    }
}
